{
    "name": "obozaxy/snicker",
    "description": "An extremely fast and eloquent php logger.",
    "type": "library",
    "keywords": [
        "snicker",
        "extremely-fast",
        "secure-by-default",
        "eloquent",
        "logger",
        "php72"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/obozaxy/snicker",
    "require": {
        "php": "^7.2",
        "obozaxy/mounds": "^1",
        "psr/container": "*",
        "psr/log": "*"
    },
    "autoload": {
        "psr-4": {
            "Snicker\\Logger\\": "./src"
        },
        "files": [
            "./loader.php",
            "./logger.php"
        ]
    },
    "suggest": {
        "obozaxy/kitkat": "Allows the data to be logged in a database.",
        "ext-json": "Allows the data to be logged in json format.",
        "ext-xml": "Allows the data to be logged in xml format."
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
