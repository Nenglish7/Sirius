{
    "name": "obozaxy/snicker",
    "description": "An extremely fast and eloquent php logger.",
    "homepage": "https://github.com/obozaxy/snicker",
    "license": "Apache-2.0",
    "type": "library",
    "keywords": [
        "snicker",
        "extremely-fast",
        "secure-by-default",
        "eloquent",
        "logger",
        "php72"
    ],
    "authors": [
        {
            "name": "Nicholas English",
            "homepage": "https://github.com/obozaxy",
            "email": "nenglish0820@outlook.com"
        },
        {
            "name": "Snicker Contributors",
            "homepage": "https://github.com/obozaxy/snicker/graphs/contributors"
        },
    ],
    "support": {
        "issues": "https://github.com/obozaxy/snicker/issues",
        "docs": "https://github.com/obozaxy/snicker/tree/master/docs"
    },
    "require": {
        "php": "^7.2",
        "psr/container": "*",
        "psr/log": "*"
    },
    "suggest": {
        "obozaxy/kitkat": "Allows the data to be logged in a database.",
        "ext-json": "Allows the data to be logged in json format.",
        "ext-xml": "Allows the data to be logged in xml format."
    },
    "require-dev": {
        "obozaxy/butterfinger": "^1",
        "phpunit/phpunit": "^7"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
